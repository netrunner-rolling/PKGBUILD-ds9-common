pkgname=default-settings-kdeapps
pkgver=(date +%Y%m%d)
pkgrel=1
pkgdesc="Default Settings for KDE Apps"
arch=('any')
url='https://github.com/ds9-common/default-settings-kdeapps'
license=('GPL')
groups=('ds9-common')
makedepends=('git')
source=('git://github.com/ds9-common/default-settings-kdeapps')
md5sums=('SKIP')

package() {
  cd $srcdir/$pkgname
  cp -aR etc $pkgdir/
  cp -aR usr $pkgdir/
}
